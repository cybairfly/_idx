pan class=3D"p">)</span> <span class=3D"p">{</span>
  <span class=3D"kd">var</span> <span class=3D"nx">result</span> <span clas=
s=3D"o">=3D</span> <span class=3D"nb">Promise</span><span class=3D"p">.</sp=
an><span class=3D"nx">resolve</span><span class=3D"p">();</span>
  <span class=3D"nx">promiseFactories</span><span class=3D"p">.</span><span=
